{
  "name": "condrado.com",
  "version": "0.0.1",
  "description": "Web de Condrado.",
  "main": "src/index.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.x"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "clean-base-html": "rimraf ./build/views/layout-base.html",
    "format": "prettier --write src/**/*.{js,json,scss,md}",
    "mock-server": "json-server --watch src/data/db.json --port 4000 --routes src/data/routes.json",
    "lint:es": "eslint --ignore-path .eslintignore \"src/**/*.js\"",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:es && npm run lint:style",
    "start:dev": "webpack-dev-server --config webpack.js -d",
    "build:dev": "rimraf build && webpack --config webpack.js -p",
    "build-minify": "webpack --config webpack.build.js && npm run clean-base-html && npm run build-pn",
    "build-minify-husky": "webpack --config webpack.test.js",
    "serve-minified": "webpack-dev-server --config webpack.test.js",
    "serve-minified-mock": "concurrently \"npm:serve-minified\" \"npm:mock-server\"",
    "build": "npm run clean && parcel build  src/javascripts/script.js src/assets/stylesheets/index.scss --no-source-maps --no-cache --no-minify"
  },
  "author": "The Cocktail",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@types/googlemaps": "^3.38.0",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flatpickr": "^4.6.3",
    "handlebars": "^4.5.3",
    "handlebars-helpers": "^0.10.0",
    "handlebars-loader": "^1.7.1",
    "handlebars-webpack-plugin": "^1.6.0",
    "husky": "^3.1.0",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "json-server": "^0.15.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.11.0",
    "node-sass-once-importer": "^5.3.2",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-handlebars": "^0.4.4",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "scrollbooster": "^2.1.0",
    "slick-carousel": "^1.8.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.5.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "browser-js": "^1.10.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jquery.scrollex": "^0.2.1",
    "jquery.scrolly": "0.0.1",
    "postcss-preset-env": "^6.7.0",
    "showdown": "^1.9.1",
    "style-loader": "^1.2.0",
    "url-loader": "^4.1.0",
    "yarn": "^1.17.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://condrado@bitbucket.org/condrado/condrado.git"
  },
  "homepage": "https://bitbucket.org/condrado/condrado#readme"
}
