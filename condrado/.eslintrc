{
  "root": true,
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
